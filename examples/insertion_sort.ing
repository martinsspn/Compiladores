module sorts;

insertionSort : int arr[], int size -> void 
{
    int i, j, key;
    i = 1;
    while(i < size) do
    {
        key = arr[i];
        j = i - 1;
        while (j >= 0 && arr[j] > key) do
        {
            arr[j + 1] = arr[j];
            j = j - 1;
        }
        arr[j+1] = key;
        i = i + 1;
    }
}
